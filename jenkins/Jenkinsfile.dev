node('currikidev') {
    stage ('Copy H5P to currikidev') {
            echo 'Copy'
            sh "yes | rm -rf /tmp/h5p-dist2" 
            sh "yes | git clone -b develop https://github.com/ActiveLearningStudio/H5P.Distribution.git /tmp/h5p-dist2" 
            sh "yes | cp -rf /tmp/h5p-dist2/* /curriki/api/storage/app/public/h5p/libraries/"
            echo 'Copy completed'
    }
}
